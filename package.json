    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DaIgeb/rvw-model.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DaIgeb/rvw-model/issues"
  },
  "homepage": "https://github.com/DaIgeb/rvw-model#readme",
  "devDependencies": {
    "@types/jest": "^24.0.22",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.2"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "ajv": "^6.10.2",
    "moment": "^2.24.0"
  }
